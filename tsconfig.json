{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "jsx": "react-jsx",
    "module": "esnext",
    "moduleResolution": "node",
    "paths": {
      "@/*": [
        "*"
      ]
    },
    "types": ["react-native"],
    "typeRoots": [
      "./types",
      "./node_modules/@types"
    ]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ]
}
